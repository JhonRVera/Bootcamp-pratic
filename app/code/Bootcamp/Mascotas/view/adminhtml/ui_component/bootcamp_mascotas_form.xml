<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">bootcamp_mascotas_form.bootcamp_mascotas_form_data_source</item>
        </item>
        <item name="label" xsi:type="string" translate="true">Creacion de Mascota</item>
        <item name="reverseMetadataMerge" xsi:type="boolean">true</item>
    </argument>
    <settings>
        <layout>
            <navContainerName>left</navContainerName>
            <type>tabs</type>
        </layout>
        <deps>
            <dep>bootcamp_mascotas_form.bootcamp_mascotas_form_data_source</dep>
        </deps>
    </settings>
    <dataSource name="bootcamp_mascotas_form_data_source">
        <argument name="data" xsi:type="array">
            <item name="js_config" xsi:type="array">
                <item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
            </item>
        </argument>
        <settings>
            <submitUrl path="mascotas/index/save"/>
        </settings>
        <dataProvider class="Bootcamp\Mascotas\Ui\DataProvider\MascotasProvider" name="bootcamp_mascotas_form_data_source">
            <settings>
                <requestFieldName>id</requestFieldName>
                <primaryFieldName>entity_id</primaryFieldName>
            </settings>
        </dataProvider>
    </dataSource>
    <fieldset name="mascota">
        <settings>
            <label translate="true">Detalles Generales</label>
        </settings>
        <field name="entity_id" formElement="input">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">mascota</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <visible>false</visible>
            </settings>
        </field>
        <field name="nombre" formElement="input">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">nombre</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Nombre</label>
            </settings>
        </field>
    </fieldset>
</form>
